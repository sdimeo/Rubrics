{
  "version": "2.0",
  "logging": {
    "logLevel": {
      "default": "Debug",
      "Host.Function.Console": "Information", // do not remove - for aspect logging
      "Host": "Warning",
      "Function": "Warning",
      "Azure": "Warning",
      "Microsoft": "Warning"
    },
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request;Trace;Exception"
      },
      "enableDependencyTracking": true,
      "snapshotConfiguration": {
        "isEnabled": true
      }
    }
  }
}